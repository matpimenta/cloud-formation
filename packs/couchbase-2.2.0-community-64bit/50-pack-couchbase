              "  /opt/couchbase/bin/couchbase-cli server-add",
              "    -u Administrator -p ", { "Ref" : "RESTPassword" },
              "    -c ", { "Fn::GetAtt" : [ "ServerNode0", "PublicIp" ] },
              "    --server-add=`hostname -i`:8091","\n",
              
            "fi","\n",
            "/bin/sh -c \"", { "Ref" : "AfterConfigure" }, "\"\n"
            
        ]]}}
      }
      
        ,
        "DependsOn" : "WaitForServerNode0"
      
    },
   
